{
  "name": "@0y0/type",
  "version": "1.0.3",
  "description": "Type checking utils.",
  "src": "src/index.js",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "repository": "https://github.com/o0y0o/type",
  "author": "Jason Chung",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "type",
    "check",
    "validation",
    "test",
    "assert",
    "predicate",
    "utility"
  ],
  "scripts": {
    "format": "prettier --write '{*,{src,test}/**/*}.js'",
    "lint": "eslint '{*,{src,test}/**/*}.js'",
    "test": "jest",
    "build": "rollup -c --bundleConfigAsCjs",
    "pkg": "[ $(yarn info $npm_package_name version) != $npm_package_version ] && yarn publish || echo Skip publishing due to v$npm_package_version exist"
  },
  "dependencies": {
    "@babel/runtime": "^7.20.13",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@0y0/babel-preset-vanilla": "^1.1.6",
    "@0y0/eslint-config-vanilla": "^1.4.0",
    "@rollup/plugin-babel": "^6.0.3",
    "babel-jest": "^29.4.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.33.0",
    "jest": "^29.4.1",
    "prettier": "^2.8.3",
    "rollup": "^3.25.3"
  }
}
