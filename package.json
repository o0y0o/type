{
  "name": "@0y0/type",
  "version": "1.0.1",
  "description": "Type checking utils.",
  "src": "src/index.js",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "iife": "dist/index.min.js",
  "repository": "https://github.com/o0y0o/type",
  "author": "Jason Chung",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "type",
    "check",
    "validation",
    "test",
    "assert",
    "predicate",
    "utility"
  ],
  "scripts": {
    "format": "prettier --write '{*,{src,test}/**/*}.js'",
    "lint": "eslint '{*,{src,test}/**/*}.js'",
    "test": "jest",
    "build": "rollup -c",
    "pkg": "[ $(yarn info $npm_package_name version) != $npm_package_version ] && yarn publish || echo Skip publishing due to v$npm_package_version exist"
  },
  "peerDependencies": {
    "@babel/runtime": ">=7.12.0",
    "lodash": ">=4.17.0"
  },
  "devDependencies": {
    "@0y0/eslint-config-vanilla": "^1.1.2",
    "@babel/core": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@rollup/plugin-babel": "^5.2.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.20.0",
    "jest": "^26.6.3",
    "lodash": "^4.17.20",
    "prettier": "^2.2.1",
    "rollup": "^2.38.5",
    "rollup-plugin-terser": "^7.0.2"
  }
}
